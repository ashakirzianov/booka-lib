{
    "files.exclude": {
        "**/dist/**": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules/**": true
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
    },
    "editor.formatOnSave": true,
    "cSpell.words": [
        "Booka",
        "Combinators",
        "Cond",
        "Diagnoser",
        "Metas",
        "Shakirzianov",
        "Subnodes",
        "Succ",
        "Truthy",
        "Whitespaces",
        "backend",
        "booq",
        "booqs",
        "coverpage",
        "deserialize",
        "diags",
        "dups",
        "epub",
        "esnext",
        "fbtoken",
        "kinesisvideomedia",
        "multiline",
        "paren",
        "parens",
        "pphs",
        "preds",
        "preprocessed",
        "promisify",
        "quotemark",
        "reparse",
        "reparser",
        "reresult",
        "rgrove",
        "slugify",
        "submodule",
        "titlepage",
        "traum",
        "upsert",
        "whitespaced",
        "xlink",
        "xmlns",
        "zipfile"
    ],
    "typescript.preferences.importModuleSpecifier": "relative",
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
}