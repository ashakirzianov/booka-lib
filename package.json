{
  "name": "booka-lib",
  "version": "0.1.0",
  "description": "Booka library service",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint",
    "debug": "tsc -w & cross-env NODE_ENV=development nodemon dist/index.js",
    "debug-local": "tsc -w & cross-env NODE_ENV=development LOCAL=all nodemon dist/index.js",
    "serve": "node dist/index.js",
    "start": "npm run serve",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashakirzianov/booka-lib.git"
  },
  "keywords": [
    "booka",
    "library"
  ],
  "author": "Anton Shakirzianov",
  "bugs": {
    "url": "https://github.com/ashakirzianov/booka-lib/issues"
  },
  "homepage": "https://github.com/ashakirzianov/booka-lib",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.11",
    "@types/koa": "^2.0.48",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.0.39",
    "@types/mongoose": "^5.3.12",
    "@types/node": "^11.13.4",
    "@types/sharp": "^0.23.0",
    "cross-env": "^5.2.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "@types/koa__cors": "^2.2.3",
    "aws-sdk": "^2.508.0",
    "axios": "^0.19.0",
    "booka-common": "^0.14.0",
    "booka-parser": "^0.7.4",
    "booka-utils": "^0.1.6",
    "dotenv": "^8.0.0",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "mongoose": "^5.7.5",
    "sharp": "^0.25.1",
    "transliteration": "^2.1.3",
    "ts-node": "^8.0.3"
  }
}
